<UserControl x:Class="WSChina2020AppComp02.UserControls.Mark"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WSChina2020AppComp02.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="10" d:DesignWidth="10">
    <Grid>
        <Image Source="{Binding Icon}" Height="10" Width="10"/>
        <TextBlock x:Name="TypeOfService" Text="{Binding ID}" Visibility="Hidden"></TextBlock>
    </Grid>
</UserControl>
